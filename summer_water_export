var featureList = [
  
    geometry14,
    
    geometry13,
 
    geometry12,

    geometry11,
 
    geometry10,
    
    geometry9,

    geometry7,
 
    geometry6,
 
    geometry5,
  
    geometry4,

    geometry3,
 
    geometry2,

    geometry
];

//imports come in as a list rather than a collection
var fl = ee.List(featureList);

//naming each exported image sequentially, losing the detail on where it has come from in the list
var imageCount = 0;

//iterate through each feature in the list
var l = featureList.length;
for(var i=0; i<l; i++) {
  //get each individual feature
  var fea = ee.Feature(fl.get(i));
  print(fea);
  //get the geometry
  var fear = fea.geometry();
  print(fear);
  //get the coordinates, note that this is a multipolygon structure so to get individual polygons we must iterate through below
  var fearl = fear.coordinates();
  print(fearl);
  //load the image that corresponds to this feature. Note that feature names must correspond to image names
  var fean = ee.String('COPERNICUS/S1_GRD/').cat(fea.get('name'));
  print(fean);
  //selecting only HH polarization for now. Will have to add HV at some point in the future (into the same geoTIFF so that they can be paired features!)
  var image = ee.Image(fean.getInfo())
    .select('HH');
  print(image);
  
  //here's where we iterate through each polygon in the geometry
  var ll =fearl.length().getInfo();
  for(var j=0; j<ll; j++) {
    //important to define as a region so that the export is limited to that area
    var region = ee.Geometry.Polygon(fearl.get(j));
    print(region);
    //giving each exported image a unique name
    imageCount++;
    var fnp = "summerWater"+imageCount;
    //the description doesn't really matter, it is just a job name in GEE, the scale is the meters per pixel, the maxpixels needed to be increased for big regions
    Export.image.toDrive({
      image: image,
      description: "testImgSummerWater",
      folder: "summerWater",
      fileNamePrefix: fnp,
      region: region,
      scale: 10,
      maxPixels: 500000000,
      fileFormat: "GeoTIFF"
    })
  }
  

}


