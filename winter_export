var featureList = [
  
    geometry11,
 
    geometry10,
    
    geometry9,
    
    geometry8,
 
    geometry7,
 
    geometry6,
 
    geometry5,
  
    geometry4,

    geometry3,
 
    geometry2,

    geometry1,

    geometry
];

var fl = ee.List(featureList);


var fc = ee.FeatureCollection(fl);
print(fc);
var imageCount = 0;

var l = featureList.length;
for(var i=0; i<l; i++) {
  var fea = ee.Feature(fl.get(i));
  print(fea);
  var fear = fea.geometry();
  print(fear);
  var fearl = fear.coordinates();
  print(fearl);
  var fean = ee.String('COPERNICUS/S1_GRD/').cat(fea.get('name'));
  print(fean);
  var image = ee.Image(fean.getInfo())
    .select('HH');
  print(image);
  
  var ll =fearl.length().getInfo();
  for(var j=0; j<ll; j++) {
    var region = ee.Geometry.Polygon(fearl.get(j));
    print(region);
    imageCount++;
    var fnp = "WinterIce"+imageCount;
    Export.image.toDrive({
      image: image,
      description: "WinterIce",
      folder: "WinterIce",
      fileNamePrefix: fnp,
      region: region,
      scale: 10,
      maxPixels: 500000000,
      fileFormat: "GeoTIFF"
    })
  }
  
  //var regions = ee.Geometry.MultiPolygon(fea);
  //print(regions);
  /*

*/
}



